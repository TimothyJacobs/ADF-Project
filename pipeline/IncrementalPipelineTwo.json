							"value": "SELECT s.CustomerName, s.ProductName, s.Quantity, s.OrderAmount, s.InsertedDate\nFROM StagingOrders s\nWHERE s.InsertedDate >  '@{activity('Lookup1').output.firstRow.InsertedDate}'\n",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
